{
  "name": "pictor-98",
  "version": "1.0.0",
  "description": "A random image and greeting generator",
  "license": "ISC",
  "author": "Jennessa Sierra",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm-run-all clean build:ts",
    "build:ts": "tsc && tsc-alias",
    "start": "node dist/app.js",
    "type-check": "tsc --noEmit",
    "start-server": "nodemon --watch src --exec tsx src/app.ts",
    "dev": "npm-run-all --parallel type-check start-server"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.1",
    "@types/method-override": "^3.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.13",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "tsc-alias": "^1.8.11",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}
